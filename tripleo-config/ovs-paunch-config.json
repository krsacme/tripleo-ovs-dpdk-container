{
    "openvswitch_db_server": {
        "environment": [
            "KOLLA_CONFIG_STRATEGY=COPY_ALWAYS"
        ],
        "image": "10.60.19.51:5000/rhosp12/centos-binary-openvswitch-db-server:latest",
        "net": "host",
        "privileged": false,
        "restart": "always",
        "volumes": [
            "/etc/hosts:/etc/hosts:ro",
            "/etc/localtime:/etc/localtime:ro",
            "/dev/log:/dev/log",
            "/etc/ssh/ssh_known_hosts:/etc/ssh/ssh_known_hosts:ro",
            "/sys/fs/selinux:/sys/fs/selinux",
            "/var/lib/kolla/config_files/openvswitch_db_server.json:/var/lib/kolla/config_files/config.json:ro",
            "/etc/openvswitch:/etc/openvswitch",
            "/run:/run",
            "/var/lib/vhost_sockets:/var/lib/vhost_sockets",
            "/var/log/containers/openvswitch/:/var/log/openvswitch/"
        ]
    },
    "openvswitch_vswitchd": {
        "environment": [
            "KOLLA_CONFIG_STRATEGY=COPY_ALWAYS"
        ],
        "image": "10.60.19.51:5000/rhosp12/centos-binary-openvswitch-vswitchd:latest",
        "net": "host",
        "privileged": true,
        "restart": "always",
        "volumes": [
            "/etc/hosts:/etc/hosts:ro",
            "/etc/localtime:/etc/localtime:ro",
            "/dev/log:/dev/log",
            "/etc/ssh/ssh_known_hosts:/etc/ssh/ssh_known_hosts:ro",
            "/sys/fs/selinux:/sys/fs/selinux",
            "/var/lib/kolla/config_files/openvswitch_vswitchd.json:/var/lib/kolla/config_files/config.json:ro",
            "/run:/run",
            "/var/lib/vhost_sockets:/var/lib/vhost_sockets",
            "/var/log/containers/openvswitch/:/var/log/openvswitch/",
            "/dev:/dev",
            "/lib/modules:/lib/modules:ro"
        ]
    }
}